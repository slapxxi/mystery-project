{
  "name": "mystery-project",
  "version": "0.0.1",
  "description": "Mystery project that exercises a wide variety of frontend development skills.",
  "main": "index.js",
  "repository": "https://github.com/slavapavlutin/mystery-project",
  "scripts": {
    "start": "NODE_ENV=production node build/server/server.js",
    "start:debug": "DEBUG=express:* NODE_ENV=production node --inspect build/server/server.js",
    "build": "yarn run build:server && yarn run build:client",
    "build:server": "tsc --project tsconfig.server.json || exit 0",
    "build:client": "next build",
    "debug": "yarn run build:server && node --inspect=9229 build/server/server.js",
    "dev": "yarn run build:server && node build/server/server.js",
    "debug:watch": "nodemon",
    "export": "next build && next export -o dist",
    "test": "jest",
    "test:server": "jest --config=jest.config.server.js",
    "image": "./bin/generate_low_quality_image",
    "lint-staged": "lint-staged",
    "docker": "yarn run docker:build && yarn run docker:run",
    "docker:build": "./bin/docker_build",
    "docker:run": "./bin/docker_run",
    "docker:attach": "docker exec -it server /bin/sh -c \"[ -e /bin/bash ] && /bin/bash || /bin/sh\"",
    "docker:stop": "docker container stop server",
    "docker:deploy": "env-cmd ./bin/docker_deploy",
    "analyze:server": "BUNDLE_ANALYZE=server yarn run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn run build"
  },
  "keywords": [
    "react",
    "testing",
    "spa",
    "ssr"
  ],
  "author": "Slava Pavlutin <sl.pavlutin@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@emotion/core": "^10.0.17",
    "@testing-library/react": "^9.1.4",
    "@xstate/react": "^0.7.1",
    "@xstate/test": "^0.1.0",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "@zeit/next-workers": "^1.0.0",
    "body-parser": "^1.19.0",
    "cacheable-response": "^1.6.6",
    "canvas": "^2.6.0",
    "compression": "^1.7.4",
    "dotenv": "^8.0.0",
    "emotion-theming": "^10.0.14",
    "env-cmd": "^9.0.3",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "firebase": "^6.4.2",
    "firebase-admin": "^8.4.0",
    "formik": "^1.5.8",
    "gsap": "^2.1.3",
    "isomorphic-unfetch": "^3.0.0",
    "lodash-es": "^4.17.15",
    "next": "^9.0.7",
    "next-i18next": "^1.1.0",
    "next-transpile-modules": "^2.3.1",
    "pipeline.macro": "^1.1.0",
    "pouchdb": "^7.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "serviceworker-webpack-plugin": "^1.0.1",
    "session-file-store": "^1.3.1",
    "url-parse": "^1.4.7",
    "uuid": "^3.3.3",
    "worker-loader": "^2.0.0",
    "xstate": "^4.6.7"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.2",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/gsap": "^1.20.2",
    "@types/jest": "^24.0.18",
    "@types/jest-image-snapshot": "^2.8.0",
    "@types/lodash-es": "^4.17.3",
    "@types/pouchdb": "^6.4.0",
    "@types/prettier": "^1.18.2",
    "@types/preval.macro": "^3.0.0",
    "@types/puppeteer": "^1.19.1",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "@types/session-file-store": "^1.2.1",
    "@types/sharp": "^0.22.2",
    "@types/supertest": "^2.0.8",
    "@types/svgo": "^1.2.1",
    "@types/testing-library__react": "^9.1.1",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^3.4.5",
    "@types/zeit__next-typescript": "^0.1.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-macros": "^2.6.1",
    "cross-env": "^5.2.0",
    "husky": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-image-snapshot": "^2.10.0",
    "jest-puppeteer": "^4.3.0",
    "lint-staged": "^9.2.5",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "preval.macro": "^3.0.0",
    "puppeteer": "^1.19.0",
    "sharp": "^0.23.0",
    "supertest": "^4.0.2",
    "svgo": "^1.3.0",
    "typescript": "^3.6.3",
    "worker-plugin": "^3.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-staged && yarn run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,tsx,html,css,json,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.{jpg,png}": [
      "yarn run image",
      "git add static/img/"
    ],
    "*.svg": [
      "svgo --config=svgo.config.yml --",
      "git add"
    ]
  }
}
